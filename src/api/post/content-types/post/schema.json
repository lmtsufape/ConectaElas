{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Link": {
      "type": "string"
    },
    "Uploadpost": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "comentarios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comentario.comentario",
      "mappedBy": "post"
    },
    "Categoria": {
      "type": "enumeration",
      "enum": [
        "Informativo",
        "Not√≠cia",
        "Relato"
      ],
      "required": true
    },
    "favoritos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::favorito.favorito",
      "inversedBy": "posts"
    }
  }
}
